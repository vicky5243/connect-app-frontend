<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Connect" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color.FF505050 -->
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryVariant">@color/black</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#FF0F77CA</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here.  -->
        <item name="blackTextColor">@color/white</item>
        <item name="whiteBgColor">@color/black</item>
        <item name="lightWhiteBgColor">#FF202020</item>

        <item name="lightBlueColor">#FF012030</item>
        <item name="blueColor">#FF0F77CA</item>
        <item name="darkBlueColor">#FFBBDCF6</item>

        <item name="veryLightGrayColor">#FF333333</item>
        <item name="lightGrayColor">#FF3A3A3A</item>
        <item name="grayColor">#FF333333</item>
        <item name="darkGrayColor">#FFA3A1A1</item>
        <item name="veryDarkGrayColor">#FF8A8989</item>
    </style>
</resources>